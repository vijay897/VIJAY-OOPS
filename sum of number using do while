#include <iostream>
using namespace std;

int main() {
    int num, i = 1, sum = 0;
    cout << "Enter the number of elements: ";
    cin >> num;

    int arr[num];

    cout << "Enter the numbers:" << endl;

    do {
        cin >> arr[i];
        i++;
    } while (i <= num);

    i = 1; 
    do {
        sum = sum + arr[i];
        i++;
    } while (i <= num);

    cout << "The sum of numbers: " << sum << endl;

    return 0;
}
