#include <iostream>
using namespace std;

int main() {
    int num, i = 1, sum = 0;
    cout << "Enter the number of elements: ";
    cin >> num;

    int arr[num];

    cout << "Enter the numbers:" << endl;
    while (i <= num) {
        cin >> arr[i];
        i++;
    }

    i = 1; 
    while (i <= num) {
        sum = sum + arr[i];
        i++;
    }

    cout << "The sum of numbers: " << sum << endl;

    return 0;
}
