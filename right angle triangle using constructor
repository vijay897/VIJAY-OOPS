#include<iostream>
using namespace std;
class hello{
  private:
  int num;
  public:
  hello(){
    num;
  }
  hello(int n){
  num = n;
  }
  void input(){
      cout<<"enter the number";
      cin>>num;
  }
  void cal(){
      for(int i=0;i<num;i++){
          for(int j=0;j<=i;j++){
              cout<<"*";
          }
          cout<<endl;
      }
  }
};
int main(){

    hello in;
    in.input();
    in.cal();
    
}
